        <argument
            android:name="numeroTelefonico"
            app:argType="string" />
        <argument
            android:name="fechaEntrega"
            app:argType="string" />
        <argument
            android:name="observaciones"
            app:argType="string" />
        <argument
            android:name="longitud"
            app:argType="string" />
        <argument
            android:name="latitud"
            app:argType="string" />
        <argument
            android:name="numeroSeguimiento"
            app:argType="string" />
