                app:destination="@id/locatePackageUserFragmentMap"/>
    </fragment>

    <fragment
        android:id="@+id/locatePackageUserFragmentMap"
        android:name="ues.pdm115.proyectopdm115grupo3.comprador.LocatePackageUserMap"
